---
---

[
    {% for group in site.groups %}
        {
            "summary": {{ group.content | strip_html | truncatewords: 150 | json2 }},
            "tags": [{% for tag in group.tags %}{{ tag | json2 }}{%unless forloop.last %}, {%endunless%}{%endfor%}],
            "image": {{ group.image | json2 }},
            "lat": {{group.lat}},
            "link": {{group.link | json2}},
            "location": {{group.location | json2}},
            "lng": {{group.lng}},
            "meetup_id": {{group.meetup_id}},
            "title": {{group.title | json2 }},
            "url": "{{group.url}}"
        }{% unless forloop.last%},{%endunless%}
    {% endfor %}
]