 {
	"jcr:primaryType": "nt:unstructured",
	"sling:resourceType": "sling-cms/components/editor/slingform",
	"button": "Save Event",
	"fields": {
		"jcr:primaryType": "nt:unstructured",
		"sling:resourceType": "sling-cms/components/general/container",
		"title": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Title",
			"name": "jcr:title",
			"required": true
		},
		"description": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/textarea",
			"label": "Description",
			"name": "jcr:description"
		},
		"taxonomy": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/taxonomy",
			"label": "Keywords",
			"name": "sling:taxonomy"
		},
		"canonical": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/path",
			"basePath": "/content",
			"type": "sling:Page",
			"label": "Canonical",
			"name": "canonical",
			"titleProperty": "jcr:content/jcr:title"
		},
		"featuredImage": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/path",
			"basePath": "/content",
			"type": "sling:File",
			"label": "Featured Image",
			"name": "sling:thumbnail",
			"titleProperty": "jcr:content/jcr:title"
		},
		"presenter": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Presenter",
			"name": "presenter",
			"required": true
		},
		"eventdate": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Event Date",
			"name": "eventdate",
			"type": "date",
			"required": true
		},
		"eventtime": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Event Time",
			"name": "eventtime",
			"required": true
		},
		"location": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Location",
			"name": "location",
			"required": true
		},
		"locationlink": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/path",
			"label": "Location Link",
			"name": "locationlink"
		},
		"eventtype": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/select",
			"label": "Event Type",
			"name": "eventtype",
			"options": [
				"Conference=Conference",
				"Meetup=Meetup",
				"Webinar=Webinar"
			]
		},
		"featured": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/select",
			"label": "Featured",
			"name": "featured",
			"options": {
				"yes": {
					"jcr:primaryType": "nt:unstructured",
					"label": "Yes",
					"value": "true"
				},
				"no": {
					"jcr:primaryType": "nt:unstructured",
					"label": "No",
					"value": "false"
				}
			}
		},
		"featuredTypeHint": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/hidden",
			"name": "featured@TypeHint",
			"value": "Boolean"
		},
		"published": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/select",
			"label": "Published",
			"name": "published",
			"options": {
				"yes": {
					"jcr:primaryType": "nt:unstructured",
					"label": "Yes",
					"value": "true"
				},
				"no": {
					"jcr:primaryType": "nt:unstructured",
					"label": "No",
					"value": "false"
				}
			}
		},
		"publishedTypeHint": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/hidden",
			"name": "published@TypeHint",
			"value": "Boolean"
		},
		"hideInSitemap": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/select",
			"label": "Hide in Sitemap",
			"name": "hideInSitemap",
			"options": {
				"no": {
					"jcr:primaryType": "nt:unstructured",
					"label": "No",
					"value": "false"
				},
				"yes": {
					"jcr:primaryType": "nt:unstructured",
					"label": "Yes",
					"value": "true"
				}
			}
		},
		"hideInSitemapTypeHint": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/hidden",
			"name": "hideInSitemap@TypeHint",
			"value": "Boolean"
		},
		"template": {
			"jcr:primaryType": "nt:unstructured",
			"sling:resourceType": "sling-cms/components/editor/fields/text",
			"label": "Template",
			"name": "sling:template",
			"disabled": true
		}
	}
}